{
  "name": "portfolio-protector",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "tsc --noEmit && deno check supabase/functions/integrated-analysis-v2/index.ts supabase/functions/integrated-analysis-v2/strategies.ts supabase/functions/chart-vision/index.ts && eslint .",
    "preview": "vite preview",
    "diagnose:analysis": "node tests/diag-integrated-analysis.js",
    "deploy:ia": "supabase functions deploy integrated-analysis --project-ref twnldqhqbybnmqbsgvpq --use-api",
    "deploy:ia-v2": "supabase functions deploy integrated-analysis-v2 --project-ref twnldqhqbybnmqbsgvpq --use-api",
    "deploy:nav-premium": "supabase functions deploy nav-premium --project-ref twnldqhqbybnmqbsgvpq --use-api",
    "deploy:etf-flows": "supabase functions deploy etf-flows --project-ref twnldqhqbybnmqbsgvpq --use-api",
    "deploy:market-context": "supabase functions deploy market-context --project-ref twnldqhqbybnmqbsgvpq --use-api",
    "deploy:chart": "supabase functions deploy chart-vision --project-ref twnldqhqbybnmqbsgvpq --use-api",
    "deploy:portfolio": "supabase functions deploy portfolio-vision --project-ref twnldqhqbybnmqbsgvpq --use-api",
    "deploy:options": "supabase functions deploy option-quote --project-ref twnldqhqbybnmqbsgvpq --use-api && supabase functions deploy option-chain --project-ref twnldqhqbybnmqbsgvpq --use-api && supabase functions deploy option-expirations --project-ref twnldqhqbybnmqbsgvpq --use-api",
    "deploy:all": "npm run deploy:ia && npm run deploy:chart && npm run deploy:portfolio && npm run deploy:options",
    "test:functions": "npm run diagnose:analysis"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
